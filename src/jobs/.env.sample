# .env.sample

# Ethereum JSON RPC URL (ex.: Infura ou Alchemy)
RPC_URL=https://your-ethereum-node-url

# Chave privada da carteira para assinar transações
PRIVATE_KEY=your_private_key

# Endereço do contrato Oracle Nrisimhadeva
NRISIMHADEVA_ORACLE_ADDRESS=0xYourOracleContractAddress

# Configuração da API do PIX
PIX_API_URL=https://pix-api.example.com
PIX_API_AUTH=your_api_bearer_token